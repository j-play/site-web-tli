<?xml version="1.0" encoding="UTF-8"?>

<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    
    <xs:element name="listepathologie" type="listepathologie"/>
    <xs:complexType name="listepathologie">
		<xs:sequence>
			<xs:element name="pathologie" type="pathologie" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
    
    <xs:complexType name="pathologie">
		<xs:all>
			<xs:element name="meridien" type="xs:string" minOccurs="1" maxOccurs="1" />
			<xs:element name="description" type="xs:string" minOccurs="1" maxOccurs="1" />
            <xs:element name="listesymptomes" type="symptomes" minOccurs="1" maxOccurs="1" />
		</xs:all>
		<xs:attribute name="type" type="type" />
		<xs:attribute name="caracteristique" type="caracteristique" />
	</xs:complexType>
    
    <xs:simpleType name="type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="m" />
			<xs:enumeration value="l" />
			<xs:enumeration value="tf" />
			<xs:enumeration value="l2" />
            <xs:enumeration value="j" />
		</xs:restriction>
	</xs:simpleType>
    
    <xs:simpleType name="caracteristique">
		<xs:restriction base="xs:string">
			<xs:enumeration value="p" />
			<xs:enumeration value="c" />
			<xs:enumeration value="v" />
			<xs:enumeration value="f" />
            <xs:enumeration value="i" />
            <xs:enumeration value="e" />
		</xs:restriction>
	</xs:simpleType>
    
    <xs:complexType name="symptomes">
		<xs:sequence>
			<xs:element name="symptome" type="xs:string"  maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>   
</xs:schema>